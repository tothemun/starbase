{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "linebreak-style": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "import/no-unresolved": 0,
    "no-undef": 0,
    "class-methods-use-this": 0
  },
  "globals": {
    "document": true,
    "window": true,
    "location": true
  }
}
